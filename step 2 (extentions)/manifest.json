{
  "manifest_version": 2,
  "name": "learning extentions bck",
  "version": "1.1",
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
    "default_title": "Click Me!",
    "default_icon": "muscle.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
